<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:dither="true" android:shape="oval">
            <solid android:color="@color/main_highlight_g7_apple" />
        </shape>
    </item>
    <item  android:state_pressed="false">
        <shape android:dither="true" android:shape="oval">
            <stroke android:width="3dp" android:color="@color/main_highlight_g7_apple" />
        </shape>
    </item>
</selector>